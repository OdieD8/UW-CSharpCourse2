<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="300" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="images/world.png"></ImageBrush>
    </Window.Background>
    <StackPanel>
        <TextBlock Height="20" Width="110" HorizontalAlignment="Left">
            Enter web address:
            <TextBlock.Background>
                <SolidColorBrush Color="White" Opacity="0.7"></SolidColorBrush>
            </TextBlock.Background>
        </TextBlock>
        <TextBox x:Name="urlBox" Height="20" VerticalAlignment="Top"></TextBox>
        <Button x:Name="goButton" Height="20" VerticalAlignment="Top" Background="#FF75A0FD" Click="goButton_Click">GO!</Button>
    </StackPanel>
</Window>
